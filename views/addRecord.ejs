<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <form action="/addRecord" method="POST">
        <label for="first_name">Enter First Name: </label>
        <input type="text" id="first_name" name="first_name">
        <br><br>
        <label for="last_name">Enter Last Name: </label>
        <input type="text" id="last_name" name="last_name">
        <br><br>
        <label for="date_missing">Enter date missing: </label>
        <input type="date" id="date_missing" name="date_missing">
        <br><br>
        <label for="age_at_missing">Enter age at missing: </label>
        <input type="number" id="age_at_missing" name="age_at_missing">
        <br><br>
        <label for="city">Enter city: </label>
        <input type="text" id="city" name="city">
        <br><br>
        <label for="state">Enter state: </label>
        <input type="text" id="state" name="state">
        <br><br>
        <label for="gender">Enter gender: </label>
        <input type="text" id="gender" name="gender">
        <br><br>
        <label for="race">Enter race: </label>
        <input type="text" id="race" name="race">
        <br><br>

        <button type="submit">Submit</button>
    </form>
</body>
</html>
